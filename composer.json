{
    "name": "lingyun/think-assistor",
    "type": "think-extend",
    "description": "ThinkPHP6 composer extension package development assistor.<br/>thinkphp6 扩展包开发的一些辅助工具。",
    "homepage": "https://github.com/ouyanglingyun",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "ouyanglingyun",
            "email": "582194733@qq.com"
        }
    ],
    "require": {
        "topthink/framework": "^6.0||^8.0",
        "lingyun/think-migration": "^3.0",
        "nunomaduro/termwind": "^1.15"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "think\\assistor\\": "src"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "extra": {
        "think": {
            "services": [
                "think\\assistor\\Service"
            ]
        }
    },
    "minimum-stability": "dev"
}